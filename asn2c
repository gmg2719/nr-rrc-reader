#!/usr/bin/env bash

rm -rf nr-rrc && mkdir nr-rrc

/usr/local/bin/asn1c \
    -fcompound-names \
    -findirect-choice \
    -gen-PER \
    -pdu=all \
    -no-gen-example \
    -fincludes-quoted \
    -funnamed-unions \
    -D nr-rrc 38331-f60.asn

